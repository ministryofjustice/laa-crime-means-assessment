{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "apiAdministrativeArea.json",
  "type": "object",
  "title": "The administrative area schema",
  "description": "Details about an administrative area",
  "properties": {
    "id": {
      "type": "integer",
      "description": "The area type id"
    },
    "code": {
      "type": "string",
      "description": "The area code"
    },
    "description": {
      "type": "string",
      "description": "The area description"
    },
    "enabled": {
      "type": "string",
      "description": "Determines whether the area is enabled"
    }
  },
  "additionalProperties": true,
  "required": ["id", "code", "enabled"]
}
