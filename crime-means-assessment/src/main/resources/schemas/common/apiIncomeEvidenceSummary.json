{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "apiIncomeEvidenceSummary.json",
  "type": "object",
  "title": "The Income Evidence Summary schema",
  "description": "Details of income evidence",
  "properties": {
    "evidenceDueDate": {
      "type": "string",
      "description": "Evidence Due Date",
      "format": "date-time"
    },
    "evidenceReceivedDate": {
      "type": "string",
      "description": "Evidence received Date",
      "format": "date-time"
    },
    "upliftAppliedDate": {
      "type": "string",
      "description": "Uplift Applied Date",
      "format": "date-time"
    },
    "upliftRemovedDate": {
      "type": "string",
      "description": "Uplift Removed Date",
      "format": "date-time"
    },
    "firstReminderDate": {
      "type": "string",
      "description": "first reminder Date",
      "format": "date-time"
    },
    "secondReminderDate": {
      "type": "string",
      "description": "second reminder Date",
      "format": "date-time"
    },
    "incomeEvidenceNotes": {
      "type": "string",
      "description": "Income Evidence Notes"
    },
    "incomeEvidence": {
      "type": "array",
      "minItems": 0,
      "items": {
        "$ref": "apiIncomeEvidence.json"
      }
    }
  },
  "additionalProperties": true
}
