{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/laa/crime/assessment/apiAssessmentSummary.json",
  "type": "object",
  "description": "Assessment Summary",
  "properties": {
    "applicantAnnualTotal": {
      "type": "number",
      "description": "Applicant Annual Total"
    },
    "partnerAnnualTotal": {
      "type": "number",
      "description": "Partner Annual Total"
    },
    "annualTotal": {
      "type": "number",
      "description": "Annual Total"
    },
    "assessmentDetail": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "http://justice.gov.uk/laa/crime/assessment/common/apiAssessmentDetail.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "applicantAnnualTotal",
    "annualTotal"
  ]
}
